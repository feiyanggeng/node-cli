{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "../../dist/kiwi",
    "lib": [
      "es6",
      "es2016.array.include"
    ],
    "skipLibCheck": true,
    "sourceMap": true,
    "types": [ "node" ],
    "typeRoots": [
      "./node_modules/@types"
    ],
    "rootDir": "./",
    "allowJs": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
  ]
}